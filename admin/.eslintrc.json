{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "linebreak-style": 0,
    "indent": "off",
    "dot-notation": "off",
    "padded-blocks": "off",
    "no-underscore-dangle": "off",
    "object-curly-spacing": "off",
    "class-methods-use-this": "off",
    "import/no-unresolved": [
      2
    ]
  }
}
