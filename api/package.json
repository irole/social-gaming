{
  "name": "irole-express-api",
  "version": "1.0.0",
  "description": "This is a Template for Api Project",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc --w",
    "start-dev": "SET NODE_ENV=development&& nodemon ./build/app/bin/www",
    "start-pro": "SET NODE_ENV=production&& node ./build/app/bin/www",
    "start-pm2-pro": "SET NODE_ENV=production&& pm2 start ./build/app/bin/www.js --name admin",
    "start-dev-server": "NODE_ENV=development nodemon ./build/app/bin/www",
    "start-pro-server": "NODE_ENV=production node ./build/app/bin/www",
    "start-pm2-pro-server": "NODE_ENV=production pm2 start ./build/app/bin/www.js --name admin",
    "mocha-ts": "SET NODE_ENV=test&& mocha -r ts-node/register test/hooks.test.ts --require mocha-steps --timeout 1000000000 --exit",
    "mocha-ts-server": "NODE_ENV=test mocha -r ts-node/register test/hooks.test.ts --require mocha-steps --timeout 1000000000 --exit",
    "eslint": "eslint typeScript/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irole/iRole-Express-Api.git"
  },
